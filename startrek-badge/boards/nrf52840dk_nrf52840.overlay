/ {
	aliases {
		touch-button = &button2;
		i2s-dev = &i2s0;
	};
};

&pinctrl {
	i2s0_default: i2s0_default {
		group1 {
			psels = <NRF_PSEL(I2S_SCK_M, 1, 1)>,
				<NRF_PSEL(I2S_LRCK_M, 1, 2)>,
				<NRF_PSEL(I2S_SDOUT, 1, 4)>,
				<NRF_PSEL(I2S_SDIN, 1, 3)>;
		};
	};
};

&i2s0 {
	status = "okay";
	pinctrl-0 = <&i2s0_default>;
	pinctrl-names = "default";
};
